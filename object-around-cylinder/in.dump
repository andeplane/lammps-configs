shell "mkdir -p fluid wall solid ppm.gnfluid ppm.fluid"

dump dump_fluid gfluid custom ${dump_freq} fluid/d.*.dump id type xs ys zs vx vy vz
dump_modify  dump_fluid   pad 8

dump dump_solid gsolid atom ${dump_freq} solid/d.*.dump
dump_modify  dump_solid   pad 8

dump dump_wall gwall atom ${wall_freq} wall/d.*.dump
dump_modify  dump_wall    pad 8

dump ppm_gnfluid gnfluid image ${dump_freq} ppm.gnfluid/dump.*.ppm type type
dump_modify ppm_gnfluid pad 8

dump ppm_fluid   gfluid image ${dump_freq} ppm.fluid/dump.*.ppm type type
dump_modify ppm_fluid pad 8
