shell "mkdir -p fluid wall solid ppm"

dump dump_fluid gfluid custom ${wall_freq} fluid/d.*.dump id type xs ys zs vx vy vz
dump_modify  dump_fluid   pad 8

dump dump_solid gsolid atom ${dump_freq} solid/d.*.dump
dump_modify  dump_solid   pad 8

dump dump_wall gwall atom ${dump_freq} wall/d.*.dump
dump_modify  dump_wall    pad 8

dump dump_img gnfluid image ${dump_freq} ppm/dump.*.ppm type type
dump_modify dump_img pad 8
